# .env.example

# Node environment setting
NODE_ENV=development

# Port for local server to run on
PORT=3000

# Skyscanner API key for testing purposes (replace with actual public key for testing)
SKYSCANNER_API_KEY=your_sky_scanner_api_key

# Session Configuration
SESSION_SECRET=your_session_secret_here
SESSION_EXPIRY=86400000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRY=3600000

# External API Keys
SKYSCANNER_API_KEY=your_skyscanner_api_key_here

